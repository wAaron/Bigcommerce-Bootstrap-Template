<div class="Block Moveable Panel" id="ChooseShippingAddress">
	<div class="header">
		<h2>%%GLOBAL_CheckoutShippingTitle%%</h2>
		%%Discount.FreeShippingEligibility%%
		<a href="%%GLOBAL_ShopPathSSL%%/account.php?action=add_shipping_address&amp;address_type=&amp;from=account.php%3Faction%3Daddress_book" class="btn btn-primary pull-right">Add Address</a>
	</div>
	<div class="BlockContent">
		<div class="TabContainer">
		fff
			<ul class="TabNav" style="%%GLOBAL_HideShippingTabs%%">
				<li id="AddressSingle" class="%%GLOBAL_ActiveTabSingle%%"><a href="%%GLOBAL_ShopPathSSL%%/checkout.php?action=choose_shipping_address&amp;type=single">%%LNG_ShipAllItemsToOneAddress%%</a></li>
				<li id="AddressMultiple" class="%%GLOBAL_ActiveTabMultiple%%"><a href="%%GLOBAL_ShopPathSSL%%/checkout.php?action=choose_shipping_address&amp;type=multiple">%%LNG_ShipAllItemsToDifferentAddresses%%</a></li>
			</ul>

			<div class="TabContent" id="TabContentAddressSingle" style="%%GLOBAL_HideTabSingle%%">
				<p class="alert alert-info">
	    			    %%GLOBAL_CheckoutShippingIntro%%
		    		</p>
				<div class="PL20">
					<ul class="ShippingAddressList">
						%%SNIPPET_ShippingAddressList%%
					</ul>
				</div>
			</div>

			<div class="TabContent" id="TabContentAddressMultiple" style="%%GLOBAL_HideTabMultiple%%">
				<p class="alert alert-info">
					%%GLOBAL_CheckoutMultiShippingIntro%%
				</p>
				<form method="post" action="%%GLOBAL_ShopPathSSL%%/checkout.php?action=save_multiple_shipping_addresses" id="multiAddressForm">
					<table class="General Stylize CartContents" cellspacing="0" cellpadding="0" border="0">
						<thead>
							<tr>
								<th class="shipItem">%%LNG_Item%%</th>
								<th class="shipAddress">%%LNG_ShippingAddress%%</th>
							</tr>
						</thead>
						<tbody>
							%%SNIPPET_MultiShippingItem%%
						</tbody>
					</table>

					<input type="submit" class="btn btn-primary pull-right" value="%%LNG_ShipToTheseAddresses%%" />
				</form>
			</div>
		</div>
	</div>
</div>

%%Panel.ChooseShippingAddressJavaScript%%

